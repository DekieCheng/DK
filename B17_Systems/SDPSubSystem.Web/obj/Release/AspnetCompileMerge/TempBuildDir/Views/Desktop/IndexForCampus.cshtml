    function formatter(arr) {
        $.each(arr, function (i, item) {
            //debugger;
            item.css('color', '#4169E1');
            //item.css("border-bottom", "1px solid #FF3E96");
            $a = $("<a onclick='' style='cursor:pointer;' >" + item.text() + "</a>");
            item.html('');
            item.append($a);
            item.find("a").css("text-decoration", "underline");

            item.click(function () {
                getFailedData(item);
            });
        });
    }

