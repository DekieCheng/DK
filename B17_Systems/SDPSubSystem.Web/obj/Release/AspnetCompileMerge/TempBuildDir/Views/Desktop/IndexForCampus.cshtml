                    label: {
                        normal: {
                            show: false,
                            //position: 'inside',
                            formatter: function (params) {
                                return params.data + '%';
                            }
                        }
                    }
                }
            ]
        }

        chart.setOption(option);
        return chart;
    }

</script>
