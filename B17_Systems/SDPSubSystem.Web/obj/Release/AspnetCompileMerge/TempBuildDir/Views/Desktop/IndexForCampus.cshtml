    function getFailedData(_this) {
        //debugger;
        //获取参数的值
        var source = '';
        var i = _this.parent().parent().find("tr").index(_this.parent()[0]);
        var category = '';
        if (i < 5) {
            source = 'AD';
        } else {
            source = 'nonAD';
        }

        var colnumber = _this.index();
        var ind = 0;
        if (i == 0 || i == 5) {
            category = $("table:eq(0) tbody tr:eq(" + i + ") td:eq(1)").text();

            if (colnumber == 3) {
                ind = 2;
            }
            else if (colnumber == 6) {
                ind = 3;
            }
            else if (colnumber == 9) {
                ind = 4;
            }
            else if (colnumber == 12) {
                ind = 5;
            }
            else if (colnumber == 15) {
                ind = 6;
            }
            else if (colnumber == 18) {
                ind = 7;
            }
        } else {
            category = $("table:eq(0) tbody tr:eq(" + i + ") td:eq(0)").text();

            if (colnumber == 2) {
                ind = 2;
            }
            else if (colnumber == 5) {
                ind = 3;
            }
            else if (colnumber == 8) {
                ind = 4;
            }
            else if (colnumber == 11) {
                ind = 5;
            }
            else if (colnumber == 14) {
                ind = 6;
            }
            else if (colnumber == 17) {
                ind = 7;
            }
        }


        var building = $("table:eq(0) thead tr:eq(0) th:eq(" + ind + ")").text();

        //var date = nowDateFormat();

        //debugger;

        //传值
        window.open('/Desktop/GetDesktopFailedDataForCampus?source=' + source + '&category=' + category + '&building=' + building, 'top');
    }

